@page "/"
@inject UserRegistrationService UserRegistrationService
<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>
<h3>@UserRegistrationService.GetCurrentUserString()</h3>

<ul>
    @foreach (var person in people)
    {
        <li>
            <p>@person</p>
        </li>
    }
</ul>


<p>@people</p>

Welcome to your new app.

@code
{
	Person[] people = CsvManager.getFromFile();
}
